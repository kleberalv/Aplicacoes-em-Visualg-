algoritmo "Exercício 5)"
// Autor :Kleber Alves
// Data : 14/08/2017
var
  produto:real

inicio

  Escreval("Digite o valor do produto à sofrer um desconto de 10% e pressione ENTER : ")
  leia (produto)
  produto:= produto - (produto*0.10)
  escreva ("O valor do produto com desconto é: ", produto)
  escreval
  escreva ("Obrigado!")

fimalgoritmo
